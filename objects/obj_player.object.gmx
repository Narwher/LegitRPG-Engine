          <argument>
            <kind>1</kind>
            <string>/// Moving
if global.gameMove{
    if left{  
        if running{
            scale = -1;
            dir = 4;
            image_speed = .17
            
            if place_free(x - 2,y){
                x -= 2;
                stepCount += 1;
            }
        }else{
            scale = -1;
            dir = 4;
            image_speed = .12
            
            if place_free(x - 1,y){
                x -= 1;
                stepCount += 1;
            }
        }
    }else if right{
        if running{
            scale = 1;
            dir = 2;
            image_speed = .17
            
            if place_free(x + 2,y){
                x += 2;
                stepCount += 1;
            }
        }else{
            scale = 1;
            dir = 2;
            image_speed = .12
            
            if place_free(x + 1,y){
                x += 1;
                stepCount += 1;
            }
        }
    }else if up{  
        if running{
            scale = 1;
            dir = 1;
            image_speed = .17
            
            if place_free(x,y - 2){
                y -= 2;
                stepCount += 1;
            }
        }else{
            scale = 1;
            dir = 1;
            image_speed = .12
            
            if place_free(x,y - 1){
                y -= 1;
                stepCount += 1;
            }
        }
    }else if down{  
        if running{
            scale = 1;
            dir = 3;
            image_speed = .17
            
            if place_free(x,y + 2){
                y += 2;
                stepCount += 1;
            }
        }else{
            scale = 1;
            dir = 3;
            image_speed = .12
            
            if place_free(x,y + 1){
                y += 1;
                stepCount += 1;
            }
        }
    }else{
        image_index = 0;
        image_speed = 0;
    }
}else{
    image_index = 0;
    image_speed = 0;
}

if dir == 1{curSprite = spr_ow_up}
else if dir == 2 {curSprite = spr_ow_lr}
else if dir == 4 {curSprite = spr_ow_lr}
else if dir == 3{curSprite = spr_ow_down}
</string>
