    if down &amp;&amp; canMove {canMove = false; vert++; alarm[0] = 5;} 

    if horz &lt; 12 &amp;&amp; horz &gt; 7{
        if vert &lt; 0 {menu = 1; vert = 3; horz = 0;}
        if vert &gt; 5 {menu = 1; vert = 3; horz = 0;}
