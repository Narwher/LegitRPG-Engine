        if vert &lt; 0 vert = 5;
        if vert &gt; 5 vert = 0;
    }   
}else{
    //draw_sprite(spr_cursor,0, (view_wview/2+78)+(horz * 42),view_hview-52);
    draw_sprite_ext(spr_finger,0, (view_wview/2+58)+(horz * 42),view_hview-46,.5,.5,image_angle,image_blend,image_alpha);
    
